{% extends 'base.html.twig' %}

{% block title %}Technology{% endblock %}

{% block body %}
    <h1>Technology</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ technology.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ technology.name }}</td>
            </tr>
            <tr>
                <th>Logo</th>
                <td>{{ technology.logo }}</td>
            </tr>
            <tr>
                <th>Version</th>
                <td>{{ technology.version }}</td>
            </tr>
            <tr>
                <th>Mastery_level</th>
                <td>{{ technology.masteryLevel }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_technology_index') }}">back to list</a>

    <a href="{{ path('app_technology_edit', {'id': technology.id}) }}">edit</a>

    {{ include('technology/_delete_form.html.twig') }}
{% endblock %}
